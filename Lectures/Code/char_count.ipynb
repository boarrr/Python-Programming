    "A = input('please enter a word:')\n",
    "C = input('please enter a character:')\n",
    "\n",
    "position = 0\n",
    "char_count = 0\n",
    "N = len(A)\n",
    "while position < N:\n",
    "    if(A[position] == C):\n",
    "        char_count = char_count+1\n",
    "    position = position + 1\n",
    "\n",
    "print(char_count)\n",
